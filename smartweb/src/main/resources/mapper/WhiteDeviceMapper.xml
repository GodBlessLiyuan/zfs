<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.rpa.web.mapper.WhiteDeviceMapper">
  <resultMap id="BaseResultMap" type="com.rpa.web.pojo.WhiteDevicePO">
    <result column="device_id" jdbcType="BIGINT" property="deviceId" />
    <result column="extra" jdbcType="CHAR" property="extra" />
    <result column="status" jdbcType="TINYINT" property="status" />
    <result column="a_id" jdbcType="INTEGER" property="aId" />
    <result column="create_time" jdbcType="TIMESTAMP" property="createTime" />
    <result column="update_time" jdbcType="TIMESTAMP" property="updateTime" />
  </resultMap>
  <insert id="insert" parameterType="com.rpa.web.pojo.WhiteDevicePO">
    insert into t_white_device (device_id, extra, `status`, 
      a_id, create_time, update_time
      )
    values (#{deviceId,jdbcType=BIGINT}, #{extra,jdbcType=CHAR}, #{status,jdbcType=TINYINT}, 
      #{aId,jdbcType=INTEGER}, #{createTime,jdbcType=TIMESTAMP}, #{updateTime,jdbcType=TIMESTAMP}
      )
  </insert>
  <insert id="insertSelective" parameterType="com.rpa.web.pojo.WhiteDevicePO">
    insert into t_white_device
    <trim prefix="(" suffix=")" suffixOverrides=",">
      <if test="deviceId != null">
        device_id,
      </if>
      <if test="extra != null">
        extra,
      </if>
      <if test="status != null">
        `status`,
      </if>
      <if test="aId != null">
        a_id,
      </if>
      <if test="createTime != null">
        create_time,
      </if>
      <if test="updateTime != null">
        update_time,
      </if>
    </trim>
    <trim prefix="values (" suffix=")" suffixOverrides=",">
      <if test="deviceId != null">
        #{deviceId,jdbcType=BIGINT},
      </if>
      <if test="extra != null">
        #{extra,jdbcType=CHAR},
      </if>
      <if test="status != null">
        #{status,jdbcType=TINYINT},
      </if>
      <if test="aId != null">
        #{aId,jdbcType=INTEGER},
      </if>
      <if test="createTime != null">
        #{createTime,jdbcType=TIMESTAMP},
      </if>
      <if test="updateTime != null">
        #{updateTime,jdbcType=TIMESTAMP},
      </if>
    </trim>
  </insert>


  <select id="query" resultType="com.rpa.web.domain.WhiteDeviceDO">
    select w.device_id as deviceId, w.extra, w.status, w.a_id as aId, w.create_time as createTime, d.imei
    from t_white_device w
    left join t_device_imei d on w.device_id = d.device_id
    <where>
      <if test="imei != null and imei != ''">
        and d.imei like '%' #{imei} '%'
      </if>
    </where>
  </select>

  <select id="queryByDeviceId" parameterType="java.lang.Long" resultType="com.rpa.web.pojo.WhiteDevicePO">
        select device_id from t_white_device where device_id = #{deviceId}
    </select>

  <delete id="deleteByDeviceId" parameterType="java.lang.Integer">
        delete from t_white_device
        where device_id = #{deviceId,jdbcType=INTEGER}
    </delete>
</mapper>